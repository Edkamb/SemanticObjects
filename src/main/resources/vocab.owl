
######## Classes
####  Semantic static layer
:MOXClass                rdf:type owl:Class.
:MOXField                rdf:type owl:ObjectProperty.
:MOXMethod               rdf:type owl:Class.

####  Semantic dynamic layer
:MOXObject               rdf:type owl:Class.
:MOXProcess              rdf:type owl:Class.

####  Syntactic layer
#Statements
:MOXStatement            rdf:type owl:Class.
:MOXGuardedStatement     rdfs:subClassOf  :MOXStatement.
:MOXValueStatement       rdfs:subClassOf  :MOXStatement.
:MOXQueryStatement       rdfs:subClassOf  :MOXStatement.
:MOXTargetStatement      rdfs:subClassOf  :MOXStatement.
:MOXVarLengthStatement   rdfs:subClassOf  :MOXStatement.
:MOXDebugStatement       rdfs:subClassOf  :MOXStatement.
:MOXPrintStatement       rdfs:subClassOf  :MOXStatement.
:MOXSequenceStatement    rdfs:subClassOf  :MOXStatement.
:MOXSkipStatement        rdfs:subClassOf  :MOXStatement.
:MOXCallStatement        rdfs:subClassOf  :MOXVarLengthStatement.
:MOXCreateStatement      rdfs:subClassOf  :MOXVarLengthStatement.
:MOXSparqlStatement      rdfs:subClassOf  :MOXVarLengthStatement.
:MOXIfStatement          rdfs:subClassOf  :MOXGuardedStatement.
:MOXWhileStatement       rdfs:subClassOf  :MOXGuardedStatement.
:MOXAssignStatement      rdfs:subClassOf  :MOXValueStatement.
:MOXReturnStatement      rdfs:subClassOf  :MOXValueStatement.
:MOXSparqlStatement      rdfs:subClassOf  :MOXQueryStatement.
:MOXOwlStatement         rdfs:subClassOf  :MOXQueryStatement.
:MOXAssignStatement      rdfs:subClassOf  :MOXTargetStatement.
:MOXSparqlStatement      rdfs:subClassOf  :MOXTargetStatement.
:MOXOwlStatement         rdfs:subClassOf  :MOXTargetStatement.
:MOXCallStatement        rdfs:subClassOf  :MOXTargetStatement.
:MOXCreateStatement      rdfs:subClassOf  :MOXTargetStatement.
:MOXStoreReturnStatement rdfs:subClassOf  :MOXTargetStatement.


#Expressions and Locations
:MOXExpression           rdf:type owl:Class.
:MOXLiteralExpression    rdfs:subClassOf  :MOXExpression.
:MOXArithExpression      rdfs:subClassOf  :MOXExpression.
:MOXLocation             rdfs:subClassOf  :MOXExpression.
:MOXOthersVarLocation    rdfs:subClassOf  :MOXLocation.
:MOXOwnVarLocation       rdfs:subClassOf  :MOXLocation.
:MOXLocalVarLocation     rdfs:subClassOf  :MOXLocation.

#Parameters
:MOXParameter   rdf:type owl:Class.
:MOXOperand     rdf:type owl:Class.

######## Properties
####  Semantic static layer
:MOhasField     rdf:type owl:ObjectProperty ;
                rdfs:domain :MOXClass ;
                rdfs:range :MOXField .

:MOhasMethod    rdf:type owl:ObjectProperty ;
                rdfs:domain :MOXClass ;
                rdfs:range :MOXMethod .

:MOextends      rdf:type owl:ObjectProperty ;
                rdfs:domain :MOXClass ;
                rdfs:range :MOXClass .

####  Semantic dynamic layer
:MOinstanceOf   rdf:type owl:ObjectProperty ;
                rdfs:domain :MOXObject ;
                rdfs:range :MOXClass .

:MOrunsOnObject rdf:type owl:ObjectProperty ;
                rdfs:domain :MOXProcess ;
                rdfs:range :MOXObject .

:MOnextOnStack  rdf:type owl:ObjectProperty ;
                rdfs:domain :MOXProcess ;
                rdfs:range :MOXProcess .

:MOactive       rdf:type owl:ObjectProperty ;
                rdfs:domain :MOXProcess ;
                rdfs:range :MOXStatement .


####  Syntactic layer

## Object properties
:MOfirst                rdf:type owl:ObjectProperty ;
                        rdfs:domain :MOXSequenceStatement ;
                        rdfs:range :MOXStatement .
:MOsecond               rdf:type owl:ObjectProperty ;
                        rdfs:domain :MOXSequenceStatement ;
                        rdfs:range :MOXStatement .
:MOhasThenBranch        rdf:type owl:ObjectProperty ;
                        rdfs:domain :MOXIfStatement ;
                        rdfs:range :MOXStatement .
:MOhasElseBranch        rdf:type owl:ObjectProperty ;
                        rdfs:domain :MOXIfStatement ;
                        rdfs:range :MOXStatement .
:MOhasCallee            rdf:type owl:ObjectProperty ;
                        rdfs:domain :MOXCallStatement ;
                        rdfs:range :MOXLocation .
:MOhasLoopBody          rdf:type owl:ObjectProperty ;
                        rdfs:domain :MOXWhileStatement ;
                        rdfs:range :MOXStatement .
:MOhasGuard             rdf:type owl:ObjectProperty ;
                        rdfs:domain :MOXGuardedStatement ;
                        rdfs:range :MOXExpression .
:MOhasQuery             rdf:type owl:ObjectProperty ;
                        rdfs:domain :MOXQueryStatement ;
                        rdfs:range :MOXExpression .
:MOhasTarget            rdf:type owl:ObjectProperty ;
                        rdfs:domain :MOXTargetStatement ;
                        rdfs:range :MOXLocation .
:MOhasValue             rdf:type owl:ObjectProperty ;
                        rdfs:domain :MOXValueStatement ;
                        rdfs:range :MOXExpression .
:MOhasParameterValue    rdf:type owl:ObjectProperty ;
                        rdfs:domain :MOXParameter ;
                        rdfs:range :MOXExpression .
:MOhasOperandValue      rdf:type owl:ObjectProperty ;
                        rdfs:domain :MOXOperand ;
                        rdfs:range :MOXExpression .
:MOhasStmtExpr          rdf:type owl:ObjectProperty ;
                        rdfs:domain :MOXPrintStatement ;
                        rdfs:range :MOXExpression .
:MOhasExpr              rdf:type owl:ObjectProperty ;
                        rdfs:domain :MOXOthersVarLocation ;
                        rdfs:range :MOXExpression .


## Datatype properties
:MOLine                 rdf:type owl:DatatypeProperty ;
                        rdfs:domain :MOXStatement ;
                        rdfs:range xsd:integer.
:MOhasMethodName        rdf:type owl:DatatypeProperty ;
                        rdfs:domain :MOXCallStatement ;
                        rdfs:range xsd:string .
:MOhasClassName         rdf:type owl:DatatypeProperty ;
                        rdfs:domain :MOXCreateStatement ;
                        rdfs:range xsd:string .
:MOhasLiteral           rdf:type owl:DatatypeProperty ;
                        rdfs:domain :MOXLiteralExpression ;
                        rdfs:range xsd:string .
:MOhasName              rdf:type owl:DatatypeProperty ;
                        rdfs:domain :MOXLocation ;
                        rdfs:range xsd:string .
:MOhasTag               rdf:type owl:DatatypeProperty ;
                        rdfs:domain :MOXLiteralExpression ;
                        rdfs:range xsd:string .
:MOhasOp                rdf:type owl:DatatypeProperty ;
                        rdfs:domain :MOXArithExpression ;
                        rdfs:range xsd:string .
:MOhasParameterIndex    rdf:type owl:DatatypeProperty ;
                        rdfs:domain :MOXParameter ;
                        rdfs:range xsd:integer .
:MOhasOperandIndex      rdf:type owl:DatatypeProperty ;
                        rdfs:domain :MOXOperand ;
                        rdfs:range xsd:integer .
:MOhasParameter         rdf:type owl:ObjectProperty ;
                        rdfs:domain :MOXVarLengthStatement ;
                        rdfs:range :MOXParameter .
:MOhasOperand           rdf:type owl:ObjectProperty ;
                        rdfs:domain :MOXArithExpression ;
                        rdfs:range :MOXOperand .



